<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<resources>	

<!-- 
*********************************[TestConfigs]*********************************
* 表示配置项用于是否需要使能或者配置特殊的值.
* 代码将会根据这些值来决定不同的执行路径.
*********************************[TestConfigs]********************************* 
-->
<setting type="config" name="keys_MENU_HOME_BACK_BACKTOUCH_SUGAR" value="0,0,0,0,0"/> <!--Keys: if the key is support, set to "1",else "0"-->
<setting type="config" name="TP" value="0"/>                               <!--TouchPanel: "0", "1", "2" means 3 ways to test-->
<setting type="config" name="no_front_flash" value="1"/>                   <!--LED-->
<setting type="config" name="Mic_2" value="0"/>                            <!--MicRecorder-->
<setting type="config" name="FmFrequencyMode" value="0"/>                  <!--FTMFMRadio: could be "2" or "1",means freq*10 or not,default "0"-->
<setting type="config" name="DontShowChargerCurrent" value="0"/>           <!--Battery: "0"-->
<setting type="config" name="PsSimulatedByTP" value="0"/>                  <!--Alsps: if psensor is simiulated by tp, set to "1"-->
<setting type="config" name="DisableSubSpeaker" value="0"/>                <!--Speaker: dual-Speaker project should set "1",to disable sub speaker -->
<setting type="config" name="NFC_supported" value="0"/>                <!--NFC -->
<setting type="config" name="camfaccalibration" value="0"/>
<setting type="config" name="sensorhub" value="1"/>
<setting type="config" name="plsensor_manully_calibrate" value="1"/>
<setting type="config" name="isWhaleSupport" value="1"/>
<setting type="config" name="callTest_number" value="112"/>





<!-- 
*********************************[items]*********************************
* 表示此测试项是否需要支持显示在测试队列[菜单]中.
* 这里的[name]是跟classname关联,需要按下面规则定义:
* echo "com.ape.factory.${name}" |sed s/com.ape.factory.//g
* [name] : 类名简写.
* [value]:
    "0" => 不支持
    "1" => 支持
* 代码中判断逻辑:
     1. 如果xml配置为"0", ==> 不支持.
     2. 如果xml配置为"1", ==> 设备自动判断是否支持.
*********************************[items]*********************************
-->

<!-- Camera class -->
<setting type="item" name="Camera" value="1"/>
<setting type="item" name="camera2.Camera2Activity" value="1"/>
<setting type="item" name="camera2.Camera3Activity" value="0"/>

<setting type="item" name="CallTest" value="1"/>
<setting type="item" name="Keys" value="1"/>
<setting type="item" name="Version" value="1"/>
<setting type="item" name="TouchPanel" value="1"/>
<setting type="item" name="LCDcolor" value="1"/>
<setting type="item" name="LCDcontrast" value="1"/>
<setting type="item" name="Speaker" value="1"/>

<setting type="item" name="Receiver" value="1"/>
<setting type="item" name="VibratorTest" value="1"/>
<setting type="item" name="MemoryCard" value="1"/>
<setting type="item" name="SIMDetect" value="1"/>
<setting type="item" name="LED" value="1"/>

<setting type="item" name="MicRecorder" value="1"/>
<setting type="item" name="FTMFMRadio" value="1"/>
<setting type="item" name="Battery" value="1"/>
<setting type="item" name="Fingerprint" value="1"/>

<!-- Sensor class -->
<setting type="item" name="Alsps" value="1"/>
<setting type="item" name="GSensor" value="1"/>
<setting type="item" name="MSensor" value="1"/>
<setting type="item" name="Gyroscope" value="0"/>

<setting type="item" name="WiFiList" value="1"/>
<setting type="item" name="BluetoothList" value="1"/>
<setting type="item" name="GPS" value="1"/>

<setting type="item" name="HALL" value="0"/>
<setting type="item" name="OTG" value="1"/>
<setting type="item" name="Nfc" value="0"/>
<setting type="item" name="HAC" value="0"/>
<setting type="item" name="PhoneColor" value="0"/>


</resources>
